container_commands:
  01_migrate:
    command: "source /opt/python/run/venv/bin/activate && python manage.py makemigrations --noinput"
  02_createsu:
    command: "source /opt/python/run/venv/bin/activate && python manage.py migrate"
  03_collectstatic:
    command: "source /opt/python/run/venv/bin/activate && python manage.py createsu --noinput"

option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: keydex/wsgi.py